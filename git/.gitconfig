[user]
	name = Ivan Dyachkov
	email = ivand@spotify.com

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --graph --date=short --decorate --oneline
	l = log --decorate --pretty=format:\"%C(yellow)%h%C(reset) %Cgreen%ad%Creset | %s%Cred%d %Cgreen(%cr) - %C(bold)%an%Creset\" --date=short
	type = cat-file -t
	dump = cat-file -p
	added = whatchanged --diff-filter=A --pretty=format:\"%h %ad | %s%d [%an]\" --date=short
[color]
	diff = auto
	ui = auto
	status = auto
[rerere]
	enabled = 1
[core]
	abbrev = 8
[push]
	default = matching
[pull]
	rebase = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
